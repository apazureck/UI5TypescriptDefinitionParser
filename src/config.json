        "replacement": "{}$1",
        "isRegex": true,
        "regexFlags": "g"
      },
      {
        "isRegex": true,
        "searchString": "sap\\.ui\\.base\\.(?:Metadata|Interface)([^\\w])",
        "replacement": "{}$1",
        "regexFlags": "g"
      },
      {
        "isRegex": true,
        "searchString": "sap\\.ui\\.model\\.(?:Context)([^\\w])",
